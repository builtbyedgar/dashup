{
  "name": "dashup",
  "version": "1.0.0-beta.5",
  "description": "A React Dashboard Component.",
  "repository": {
    "type": "git",
    "url": "https://github.com/builtbyedgar/dashup"
  },
  "author": "Edgar Bermejo",
  "license": "MIT",
  "homepage": "http://builtbyedgar.github.io/dashup",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.js",
  "types": "dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "eslint --max-warnings 0 \"*/**/*.{ts,tsx}\" --ignore-path .gitignore",
    "format": "prettier --write . --ignore-path .gitignore",
    "precommit": "lint-staged && test",
    "test": "vitest",
    "storybook": "storybook dev -p 6006",
    "build": "npm run clean && vite build && npm run typings",
    "typings": "cp ./src/typings/types.d.ts ./dist",
    "clean": "rm -rf dist",
    "pack": "npm run build && npm pack",
    "publish": "npm publish --access public",
    "postinstall": "npm exec -y husky install",
    "build-storybook": "storybook build -o /docs",
    "deploy-storybook": "gh-pages -d storybook-static"
  },
  "keywords": [
    "ui",
    "react",
    "library",
    "component",
    "dashboard",
    "layout",
    "draggable",
    "sortable",
    "resizable"
  ],
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.1",
    "@storybook/addon-essentials": "^7.0.18",
    "@storybook/addon-interactions": "^7.0.18",
    "@storybook/addon-links": "^7.0.18",
    "@storybook/blocks": "^7.0.18",
    "@storybook/react": "^7.0.18",
    "@storybook/react-vite": "^7.0.18",
    "@testing-library/dom": "^9.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.32.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "gh-pages": "^5.0.0",
    "happy-dom": "^9.20.3",
    "husky": "^8.0.3",
    "jsdom-testing-mocks": "^1.9.0",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "sass": "^1.63.4",
    "storybook": "^7.0.18",
    "typescript": "4.9.3",
    "typescript-eslint": "0.0.1-alpha.0",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "npm run lint"
    ],
    "*.+(ts|tsx|json|css|md)": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
